<% include ./partials/header %>
  
  <!-- Header Section -->
  <header class="d-flex align-items-center justify-content-center">
    <div class="banner-center text-center">
      <h1 class="display-2 text-white mb-5">Coffee Junkie</h1>
      <div class="banner-underline mx-auto mb-5"></div>
      <a href="#skills" class="btn btn-outline-light btn-lg explore">Explore</a>
    </div>
  </header>
  <!-- End of Header Section -->
  
  
  
  <!-- Skills Section -->
  <section id="skills" class="text-center py-5">
    <div class="container-fluid">
      <div class="row mb-5">
        <div class="col">
          <div class="title">
            <h1 class="display-4 mb-3">Skills</h1>
            <div class="text-underline mb-3"></div>
            <p class="text-muted skills-text">We are passionate about everything coffee!</p>
          </div>
        </div>
      </div>
      
      <div class="row mb-4">
        <div class="col-md-4 mb-5 mb-md-0">
          <i class="fas fa-coffee fa-5x mb-3"></i>
          <h2>Quality</h2>
          <div class="skills-body">
            <p class="text-muted">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Quasi provident eligendi at, modi quas enim hic quod beatae recusandae animi.</p>
            <a href="#" class="btn btn-dark skills-btn mt-3">Learn More</a>
          </div>
        </div>
        
        <div class="col-md-4 mb-5 mb-md-0">
          <i class="fas fa-cubes fa-5x mb-3"></i>
          <h2>Innovation</h2>
          <div class="skills-body">
            <p class="text-muted">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Quasi provident eligendi at, modi quas enim hic quod beatae recusandae animi.</p>
            <a href="#" class="btn btn-dark skills-btn mt-3">Learn More</a>
          </div>
        </div>
        
        <div class="col-md-4 mb-5 mb-md-0">
          <i class="fas fa-bolt fa-5x mb-3"></i>
          <h2>Energy</h2>
          <div class="skills-body">
            <p class="text-muted">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Quasi provident eligendi at, modi quas enim hic quod beatae recusandae animi.</p>
            <a href="#" class="btn btn-dark skills-btn mt-3">Learn More</a>
          </div>
        </div>
      </div>
      
    </div>
  </section>
  <!-- End of Skills Section -->
  
  
  
  <!-- About Section -->
  <section id="about" class="text-center">
    <div class="container-fluid">
      
      <div class="row">
        <div class="col-sm-4 d-flex align-items-center my-5 my-md-0">
          <div class="title">
            <h1 class="display-4 mb-3 text-white">About</h1>
            <div class="text-underline mb-3"></div>
            <p class="about-text text-white">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Natus tenetur magni at, perferendis atque asperiores quidem deserunt dolores quas non.</p>
          </div>
        </div>
        <div class="col-sm-8 pic"></div>
      </div>
      
    </div>
  </section>
  <!-- End of About Section -->
  
  
  
  <!-- Contact Section -->
  <section id="contact" class="text-center py-5">
    <div class="container-fluid">
      
     <div class="row mb-5">
        <div class="col">
          <div class="title">
            <h1 class="display-4 mb-3">Contact</h1>
            <div class="text-underline mb-3"></div>
            <p class="text-muted skills-text">Drop us a line!</p>
          </div>
        </div>
      </div>
      
      <div class="row mb-5">
        <div class="col-lg-5 mx-auto">
          
          <form>
            <div class="form-group">
              <input type="text" class="form-control form-control-lg" id="firstname" name="firstname" placeholder="First Name">
            </div>
            <div class="form-group">
              <input type="text" class="form-control form-control-lg" id="lastname" name="lastname" placeholder="Last Name">
            </div>
            <div class="form-group">
              <input type="tel" class="form-control form-control-lg" id="phone" name="phone" placeholder="Phone Number">
            </div>
            <div class="form-group">
              <input type="email" class="form-control form-control-lg" id="email" name="email" placeholder="Email Address">
            </div>
            <button class="btn btn-dark btn-lg btn-block form-btn">Submit</button>
          </form>
          
        </div>
      </div>
      
    </div>
  </section>
  <!-- End of Contact Section -->

  
<% include ./partials/footer %>