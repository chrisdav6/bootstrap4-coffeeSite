  </section>
  <!-- End of About Section -->
  
  
  
  <!-- Contact Section -->
  <section id="contact" class="text-center py-5">
    <div class="container-fluid">
      
     <div class="row mb-5">
        <div class="col">
          <div class="title">
            <h1 class="display-4 mb-3">Contact</h1>
            <div class="text-underline mb-3"></div>
            <p class="text-muted skills-text">Drop us a line!</p>
          </div>
        </div>
      </div>
      
      <div class="row mb-5">
        <div class="col-lg-5 mx-auto">
          
          <form>
            <div class="form-group">
              <input type="text" class="form-control form-control-lg" id="firstname" name="firstname" placeholder="First Name">
            </div>
            <div class="form-group">
              <input type="text" class="form-control form-control-lg" id="lastname" name="lastname" placeholder="Last Name">
            </div>
            <div class="form-group">
              <input type="tel" class="form-control form-control-lg" id="phone" name="phone" placeholder="Phone Number">
            </div>
            <div class="form-group">
              <input type="email" class="form-control form-control-lg" id="email" name="email" placeholder="Email Address">
            </div>
            <button class="btn btn-dark btn-lg btn-block form-btn">Submit</button>
          </form>
          
        </div>
      </div>
      
    </div>
  </section>
  <!-- End of Contact Section -->
  
  
  
